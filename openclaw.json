{
  "agents": {
    "list": [
      {
        "id": "marketing",
        "default": true,
        "name": "Marketing Boss",
        "workspace": "~/.openclaw/workspace/workspace-marketing",
        "model": "openai/gpt-5.1-codex",
        "subagents": {
          "allowAgents": ["copywriter", "adanalyst"]
        }
      },
      {
        "id": "copywriter",
        "name": "Copywriter",
        "workspace": "~/.openclaw/workspace/workspace-copywriter",
        "model": "openai/gpt-4o-mini"
      },
      {
        "id": "adanalyst",
        "name": "Ad Analyst",
        "workspace": "~/.openclaw/workspace/workspace-adanalyst",
        "model": "openai/gpt-4o-mini"
      },
      {
        "id": "ops",
        "name": "Operations Boss",
        "workspace": "~/.openclaw/workspace/workspace-ops",
        "model": "openai/gpt-5.1-codex",
        "subagents": {
          "allowAgents": ["inventory", "supplierscout"]
        }
      },
      {
        "id": "inventory",
        "name": "Inventory Manager",
        "workspace": "~/.openclaw/workspace/workspace-inventory",
        "model": "openai/gpt-4o-mini"
      },
      {
        "id": "supplierscout",
        "name": "Supplier Scout",
        "workspace": "~/.openclaw/workspace/workspace-supplierscout",
        "model": "openai/gpt-4o-mini"
      },
      {
        "id": "cx",
        "name": "CX Boss",
        "workspace": "~/.openclaw/workspace/workspace-cx",
        "model": "openai/gpt-5.1-codex",
        "subagents": {
          "allowAgents": ["supportagent", "reviews"]
        }
      },
      {
        "id": "supportagent",
        "name": "Support Agent",
        "workspace": "~/.openclaw/workspace/workspace-supportagent",
        "model": "openai/gpt-4o-mini"
      },
      {
        "id": "reviews",
        "name": "Reviews Manager",
        "workspace": "~/.openclaw/workspace/workspace-reviews",
        "model": "openai/gpt-4o-mini"
      }
    ]
  },
  "tools": {
    "agentToAgent": {
      "enabled": true,
      "allow": [
        "marketing",
        "copywriter",
        "adanalyst",
        "ops",
        "inventory",
        "supplierscout",
        "cx",
        "supportagent",
        "reviews"
      ]
    }
  },
  "channels": {
    "telegram": {
      "accounts": {
        "marketingbot": {
          "botToken": "TODO_MARKETING_BOT_TOKEN"
        },
        "opsbot": {
          "botToken": "TODO_OPS_BOT_TOKEN"
        },
        "cxbot": {
          "botToken": "TODO_CX_BOT_TOKEN"
        }
      },
      "dmPolicy": "pairing"
    }
  },
  "bindings": [
    {
      "agentId": "marketing",
      "match": {
        "channel": "telegram",
        "accountId": "marketingbot"
      }
    },
    {
      "agentId": "ops",
      "match": {
        "channel": "telegram",
        "accountId": "opsbot"
      }
    },
    {
      "agentId": "cx",
      "match": {
        "channel": "telegram",
        "accountId": "cxbot"
      }
    }
  ]
}
